{
  "name": "logic2ableton",
  "version": "1.0.0",
  "description": "Convert Logic Pro projects to Ableton Live sets",
  "main": "./out/main/index.js",
  "author": {
    "name": "Tyler Eveland",
    "email": "j.tyler.eveland@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Evilander/logic2ableton.git"
  },
  "scripts": {
    "dev": "electron-vite dev",
    "build": "electron-vite build",
    "preview": "electron-vite preview",
    "postinstall": "electron-vite build",
    "dist": "electron-vite build && electron-builder --config electron-builder.yml",
    "dist:win": "electron-vite build && electron-builder --win --config electron-builder.yml",
    "dist:mac": "electron-vite build && electron-builder --mac --config electron-builder.yml"
  },
  "dependencies": {
    "@phosphor-icons/react": "^2.1.10",
    "geist": "^1.7.0",
    "motion": "^12.34.2",
    "react": "^19.2.4",
    "react-dom": "^19.2.4"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.2.0",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.4",
    "electron": "^40.6.0",
    "electron-builder": "^26.8.1",
    "electron-vite": "^5.0.0",
    "tailwindcss": "^4.2.0",
    "typescript": "^5.9.3"
  }
}
